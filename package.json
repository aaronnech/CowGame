{
  "name": "RTS",
  "version": "0.0.1",
  "description": "A HTML5 multiplayer RTS game",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "make_client": "cd js/client; ../../node_modules/.bin/tsc ./client.ts --module commonjs; cd ../..",
    "make_server": "./node_modules/.bin/tsc ./server.ts --module commonjs",
    "compile": "npm run-script make_client && npm run-script make_server",
    "serve": "node ./server.js",
    "build": "npm install && npm run-script compile && npm run-script serve"
  },
  "dependencies": {
    "express": "4.9.7",
    "mongodb": "^1.4.19",
    "socket.io": "^1.1.0",
    "body-parser": "~1.9.0"
  },
  "devDependencies": {
    "typescript": "1.1.0-1"
  },
  "author": "Aaron Nech",
  "license": "N/A"
}
